<Window x:Class="MindMap.Windows.AddChild"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MindMap.Windows"
        mc:Ignorable="d"
        Title="AddChild" Height="140" Width="300" WindowStartupLocation="CenterScreen" WindowStyle="ToolWindow" Loaded="Window_Loaded">
    <Grid>
        <StackPanel Margin="10">
            <TextBlock Foreground="Gray">Nome</TextBlock>
            <TextBox FontSize="14" Name="NomeTB" KeyDown="NomeTB_KeyDown"></TextBox>
            <Grid Margin="0,10,0,0" x:Name="AdicionarB" MouseDown="AdicionarB_MouseDown"></Grid>
        </StackPanel>
    </Grid>
</Window>
